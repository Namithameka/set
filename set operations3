#set operations
#union
a = {1, 2, 3}
b = {3, 4, 5}
print(a | b)              # {1, 2, 3, 4, 5}
print(a.union(b))
#intersection
print(a & b)              # {3}
print(a.intersection(b))
#difference
print(a - b)              # {1, 2}
print(a.difference(b))
#symmetric difference
print(a ^ b)              # {1, 2, 4, 5}
print(a.symmetric_difference(b))
